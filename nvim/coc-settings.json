{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "json",
    "markdown",
    "python",
    "html",
    "terraform"
  ],
  "explorer.width": 30,
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": false,
  "explorer.keyMappings.global": {
    "<cr>": [
      "expandable?",
      "expand",
      "open"
    ],
    "v": "open:vsplit",
    "o": [
      "expanded?",
      "collapse",
      "expand"
    ]
  }
  /* "python.autoComplete.extraPaths": [ */
